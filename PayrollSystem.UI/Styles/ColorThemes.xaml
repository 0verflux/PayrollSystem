<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryDark">#388E3C</Color>
    <Color x:Key="PrimaryMid">#4CAF50</Color>
    <Color x:Key="PrimaryLight">#C8E6C9</Color>
    <Color x:Key="Accent">#009688</Color>
    <Color x:Key="PrimaryText">#212121</Color>
    <Color x:Key="SecondaryText">#757575</Color>

    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDark}"/>
    <SolidColorBrush x:Key="PrimaryMidBrush" Color="{StaticResource PrimaryMid}"/>
    <SolidColorBrush x:Key="PrimaryLightBrush" Color="{StaticResource PrimaryLight}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}"/>
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="{StaticResource PrimaryText}"/>
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="{StaticResource SecondaryText}"/>
</ResourceDictionary>